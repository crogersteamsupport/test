<log4net>
		<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender" >
			<file value="Logs\ " />
			<appendToFile value="true" />
			<lockingModel type="log4net.Appender.RollingFileAppender+MinimalLock" />
			<rollingStyle value="Date" />
			<datePattern value ="'WebHooks_'yyyyMMdd'.log'" />
			<staticLogFileName value="false" />
			<MaxDateRollBackups value="1" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{h:mm:ss tt}: %-5level- %message%newline" />
			</layout>
		</appender>
		<appender name="RollingLogFileAppenderApp" type="log4net.Appender.RollingFileAppender" >
			<file value="Logs\ " />
			<appendToFile value="true" />
			<lockingModel type="log4net.Appender.RollingFileAppender+MinimalLock" />
			<rollingStyle value="Date" />
			<datePattern value ="'WebApp_'yyyyMMdd'.log'" />
			<staticLogFileName value="false" />
			<MaxDateRollBackups value="1" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{h:mm:ss tt}: %-5level- %message%newline" />
			</layout>
		</appender>
		<root>
			<level value="DEBUG" />
			<appender-ref ref="RollingLogFileAppender" />
			<appender-ref ref="RollingLogFileAppenderApp" />
		</root>
	</log4net>